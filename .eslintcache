tarea\n                               rows=\"4\"\n                               cols=\"32\"\n                              type=\"text\"\n                              placeholder=\"Description\"\n                              className=\"mb-2\"\n                              onChange={(e) =>\n                                setDescription({\n                                  description: e.target.value,\n                                })\n                              }\n                            />\n                            <button className=\"buttonModify\">Ok</button>\n                          </form>\n                          <form\n                            className=\"form-modify\"\n                            onSubmit={function (e) {\n                              e.preventDefault();\n\n                              handleSubmitPrice(product.id);\n                            }}\n                          >\n                            <input\n                              \n                              type=\"text\"\n                              placeholder=\"prix\"\n                              className=\"mb-2\"\n                              onChange={(e) =>\n                                setPrice({\n                                  price: e.target.value,\n                                })\n                              }\n                            />\n                            <button className=\"buttonModify\">Ok</button>\n                          </form>\n                          </div>\n                        </AccordionDetails>\n                      </Accordion>\n                    </div>\n              </div>\n            </div>\n          ))}\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default ShopDelete;\n","/home/fatima/frenchTouch/frenchtouch-front-/src/Fetch.js",[],["204","205"],"/home/fatima/frenchTouch/frenchtouch-front-/src/components/Administrateur/NavBar/Nav.jsx",["206"],"import \"./Nav.css\";\ni
               <label>\n                    Image : <span className=\"styleRequired\">*</span>\n                    <FileUpload\n                      className=\"imageForm\"\n                      method={(e) => {\n                        e.preventDefault();\n                        handleUpload();\n                      }}\n                      onChange={(e) => {\n                        onChange(e);\n                      }}\n                      fileName={uploadedFile.fileName}\n                      filePath={uploadedFile.filePath}\n                    />\n                  </label>\n                </div>\n                <p>\n                  <span className=\"styleRequired\">*</span> champs obligatoires\n                </p>\n                <button\n                  type=\"submit\"\n                  className=\"submitAdd\"\n                  value=\"Submit\"\n                  onClick={handleValid}\n                >\n                  Envoyer\n                </button>\n                <span className=\"msgValid\">{valid}</span>\n              </fieldset>\n            </form>\n          </div>\n        </div>\n      </div>\n      <DeletePalette />\n    </div>\n  );\n}\n\nexport default Prestations;\n","/home/fatima/frenchTouch/frenchtouch-front-/src/components/Administrateur/FileUpload/FileUpload.jsx",[],"/home/fatima/frenchTouch/frenchtouch-front-/src/components/visiteur/home/CommentCarousel.jsx",["223"],"import \"./CommentCarousel.css\";\nimport axios from \"axios\";\nimport { useEffect, useState } from \"react\";\nimport { FETCH } from \"../../../Fetch\";\n\nfunction CommentCarousel() {\n\n  const [name, setName] = useState(\"\");\n  const [postCode, setPostCode] = useState(\"\");\n  const [adress, setAdress] = useState(\"\");\n  const [email, setEmail] = useState(\"\");\n  const [message, setMessage] = useState(\"\");\n  const [textHidden, setTexthidden] = useState(\"\");\n\n\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n\n      axios\n        .post(`${FETCH}/notices`, {\n          name: name,\n          postCode: postCode,\n          adress: adress,\n          email: email,\n          message: message,\n        })\n        .then(function (response) {\n          window.history.go();\n\n          console.log(response);\n        })\n        .catch(function (error) {\n          verifEmpty()\n\n          console.log(error);\n        });\n  };\n\n  function verifEmpty() {\n    if (\n      name === \"\" ||\n      adress === \"\" ||\n      postCode === \"\" ||\n      email === \"\" ||\n      message === \"\"\n    ) {\n      alert(\"Des champs obligatoires sont vides.\");\n    } else {\n      setTexthidden(\n        \"Votre association a bien été recensée et sera traitée dans les plus courts délais.\"\n      );\n    }\n  }\n  return (\n    <div>\n      <h2 className=\"CommentTitle\">Laisser nous un commentaire </h2>\n\n      <div className=\"FormContact App  \">\n        <div className=\"FormulaireContact\">\n          <div className=\"form\">\n            <form className=\"contact-form \" onSubmit={handleSubmit}>\n              <div className=\"CommentCarousel \">\n                <div>\n                  <div className=\"Contact-M\">\n                    <label>\n                      <input\n                        type=\"text\"\n                        required\n                        value={name}\n                        placeholder=\"Nom\"\n                        onChange={(e) => setName(e.target.value)}\n                        className=\"form-inputComment \"\n                        name=\"user_lastname\"\n                      />\n                    </label>\n                  </div>\n                  <div className=\"Contact-M\">\n                    <label>\n                      <input\n                        type=\"text\"\n                        required\n                        value={adress}\n                        placeholder=\"Adresse\"\n                        onChange={(e) => setAdress(e.target.value)}\n                        className=\"form-inputComment \"\n                        name=\"user_firstname\"\n                      />\n                    </label>\n                  </div>\n                  <div className=\"Contact-M\">\n                    <label>\n                      <input\n                        type=\"text\"\n                        required\n                        value={postCode}\n                        placeholder=\"Code postal\"\n                        onChange={(e) => setPostCode(e.target.value)}\n                        className=\"form-inputComment \"\n                        name=\"user_firstname\"\n                      />\n                    </label>\n                  </div>\n                </div>\n                <div className=\"emailComment\">\n                  <div className=\"Contact-M\">\n                    <label>\n                      <input\n                        type=\"email\"\n                        required\n                        value={email}\n                        placeholder=\"Email\"\n                        onChange={(e) => setEmail(e.target.value)}\n                        className=\"form-inputComment \"\n                        name=\"user_email\"\n                      />\n                    </label>\n                  </div>\n\n                  <div className=\"Contact-M\">\n                    <label>\n                      <textarea\n                        name=\"message\"\n                        required\n                        value={message}\n                        placeholder=\"Message\"\n                        onChange={(e) => setMessage(e.target.value)}\n                        className=\"form-inputComment scrolls \"\n                      />\n                    </label>\n                  </div>\n                </div>\n                <div className=\"submitComment\">\n                  <input\n                    type=\"submit\"\n                    onClick={verifEmpty}\n                    className=\" btn btnComent\"\n                    value=\"Envoyer\"\n                  />\n                </div>\n              </div>\n\n              <span className=\"msgValidFormation\">{textHidden}</span>\n            </form>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default CommentCarousel;\n","/home/fatima/frenchTouch/frenchtouch-front-/src/components/visiteur/prestation/CartGift.jsx",[],["224","225"],"/home/fatima/frenchTouch/frenchtouch-front-/src/components/Administrateur/Adminhome/AcceptComment.jsx",[],"/home/fatima/frenchTouch/frenchtouch-front-/src/components/visiteur/reserve/Calendar.jsx",["226","227"],"import React from \"react\";\nimport \"./Calendar.css\";\nimport { useEffect } from \"react\";\nimport { InlineWidget } from \"react-calendly\";\n\nconst Calendar = () => {\n \n\n  return (\n    <div>\n      <div className=\"calendlyCart \">\n        <InlineWidget style=\" height: auto;\n    overflow: hidden;\" url=\"https://calendly.com/dahnini-fatima\"  />\n      </div>\n    </div>\n  );\n};\n\nexport default Calendar;\n","/home/fatima/frenchTouch/frenchtouch-front-/src/components/visiteur/shop/Products.jsx",[],"/home/fatima/frenchTouch/frenchtouch-front-/src/components/Adminlogin/AdminLogin.jsx",["228","229"],"/home/fatima/frenchTouch/frenchtouch-front-/src/components/Adminlogin/RegisterLogin.jsx",["230","231","232"],"import React, { useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport './RegisterLogin.css'\nimport { FETCH } from \"../../Fetch\";\n\nfunction RegisterLogin() {\n    const [user, setUser] = useState([]);\n    const [firstname, setFirstname] = useState(\"\");\n    const [lastname, setLastname] = useState(\"\");\n    const [email, setEmail] = useState(\"\");\n    const [password, setPassword] = useState(\"\");\n    const [confirmedPassword, setConfirmedPassword] = useState(\"\");\n\n    const [statusBtn, setStatusBtn] = useState(true);\n\n\n\n\n    \n\n    const handleSubmit = (e) => {\n\n        e.preventDefault();\n        if (password !== confirmedPassword) {\n          alert(\"le mots de passe est incorect\");\n      }\n        else if (statusBtn === true) {\n          setStatusBtn(false);\n          axios\n            .post(\n              `${FETCH}/loginUsers/register`,\n              {\n                firstname: firstname,\n                lastname : lastname,\n                email : email,\n                password : password,\n                confirmedPassword : confirmedPassword,\n\n              },\n            \n\n            )\n            .then(function (response) {\n              window.history.go();\n              console.log(response);\n            })\n            \n            .catch(function (error) {\n              console.log(error);\n            });\n        }\n      };\n    return (\n      <div>\n        <div className=\"Contact \">\n            <div className=\"margContact\">\n              <div className=\"CartContact\">\n                <div>\n                  <h3 className=\"titleContact\">Inscrivez vous</h3>\n                  <form className=\"register-form\"  onSubmit={handleSubmit} >\n         \n            <div className=\"Contact-M\">\n          <input\n            className=\"form-inputRegister\"\n            type=\"text\"\n            placeholder=\"Prénom\"\n            name=\"firstname\"\n            value={firstname}\n            onChange={(e) => setFirstname(e.target.value)}\n          />\n            </div>\n            <div className=\"Contact-M\">\n              <label>\n          <input\n          \n            required\n            className=\"form-inputRegister\"\n\n            placeholder=\"Nom\"\n            name=\"lastname\"\n            value={lastname}\n            onChange={(e) => setLastname(e.target.value)}\n          />\n             </label>\n            </div>\n        <div className=\"Contact-M\">\n              <label>\n          <input\n          \n            className=\"form-inputRegister\"\n            placeholder=\"Email \"\n            name=\"email\"\n            value={email}\n            onChange={(e) => setEmail(e.target.value)}\n            required\n\n          />\n             </label>\n            </div>\n            <div className=\"Contact-M\">\n              <label>\n          <input\n        \n            required\n            className=\"form-inputRegister\"\n            name=\"password\"\n            placeholder=\"Mot de passe\"\n            type=\"password\"\n            value={password}\n            onChange={(e) => setPassword(e.target.value)}\n          />\n            </label>\n            </div>\n            <div className=\"Contact-M\">\n              <label>\n          <input\n        \n            required\n            className=\"form-inputRegister\"\n            name=\"confirmpassword\"\n            placeholder=\"Confimer le mot de passe\"\n            type=\"password\"\n            value={confirmedPassword}\n            onChange={(e) => setConfirmedPassword(e.target.value)}\n            \n          />\n            </label>\n            </div>\n            <input\n            type=\"submit\"\n            className=\"submitContact\"\n            value=\"S'inscrire\"\n          />\n        \n        </form>\n                </div>\n              </div>\n            </div>\n      </div>\n      </div>\n    );\n  }\n  \n  export default RegisterLogin;","/home/fatima/frenchTouch/frenchtouch-front-/src/App.jsx",["233","234"],"import \"./App.css\";\nimport Home from \"./components/visiteur/home/Home\";\nimport HomeAdmin from \"./components/Administrateur/Adminhome/AdminHome\";\nimport { Route, Switch } from \"react-router-dom\";\nimport AdminLogin from \"./components/Adminlogin/AdminLogin\";\nimport Prestations from \"./components/visiteur/prestation/Presentation\";\nimport CartGifts from \"./components/visiteur/prestation/CartGift\";\nimport Reserves from \"./components/vi
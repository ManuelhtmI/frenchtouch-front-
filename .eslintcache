lassName=\"AdminHomeImage\">\n                <div className=\"adminHomeMargin\">\n                  <div className=\"imagePresentation\">\n                    <div>\n                      <img\n                        className=\"imagePresentationFrenchTouchAdmin\"\n                        src={res.picture_home}\n                        alt=\"\"\n                      />\n                    </div>\n                    <div>\n                      <Accordion>\n                        <AccordionSummary\n                          expandIcon={<ExpandMoreIcon />}\n                          aria-controls=\"\"\n                          id=\"\"\n                        >\n                          <Typography>Modifier</Typography>\n                        </AccordionSummary>\n\n                        <AccordionDetails>\n                          <div className=\"mb-2\">\n                            <form\n                              onSubmit={function (e) {\n                                e.preventDefault();\n                                onSubmitPicture(res.id);\n                              }}\n                            >\n                              <div className=\"custom-file mb-2 mt-2\">\n                                <input\n                                  required\n                                  type=\"file\"\n                                  className=\"custom-file-input \"\n                                  id=\"customFile\"\n                                  onChange={onChange}\n                                  accept=\"image/*\"\n                                />\n                                <label\n                                  className=\"custom-file-label\"\n                                  htmlFor=\"customFile\"\n                                >\n                                  {fileName}\n                                </label>\n                              </div>\n\n                              <input\n               